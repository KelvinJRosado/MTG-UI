    "@nx/node": "21.1.2",
    "@nx/vite": "21.1.2",
    "@nx/vue": "21.1.2",
    "@nx/web": "21.1.2",
    "@nx/workspace": "21.1.2",
    "@swc-node/register": "1.9.1",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.11",
    "@types/node": "18.16.9",
    "@typescript-eslint/parser": "7.16.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitest/ui": "3.0.0",
