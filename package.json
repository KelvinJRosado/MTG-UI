    "@nx/vite": "21.1.3",
    "@nx/vue": "21.1.3",
    "@nx/web": "21.1.3",
    "@nx/workspace": "21.1.3",
    "@swc-node/register": "1.10.10",
    "@swc/core": "1.11.31",
